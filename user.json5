    "PWRAO*EULD": "bridle", // but with star is fine too:   PW*UPBLD "bundle"
    "KAPBLD": "candle",
    "KA*PBLD": "candle",
    "KRAEULD": "cradle",
    "KRA*EULD": "cradle",
    "K*URLD": "curdle",  // KURLD is "curled"
    "TKA*ULD": "dawdle", // TKAULD is "called it"
    "TKAOLD": "doodle",
    "TKAO*LD": "doodle",
    "TKWEUPBLD": "dwindle",
    "TKW*EUPBLD": "dwindle",
    "TPOPBLD": "fondle",
    "TPO*PBLD": "fondle",
    "TKPWEURLD": "girdle",
    "TKPW*EURLD": "girdle",
    "AOEULD": "idle", // overrides "island"
    "AO*EULD": "idle",
    "AOEUPBLD": "island", // use SKPAOEUPL for "and I'm"
    "AO*EUPBLD": "Island",
    "KEUPBLD": "kindle",
    "K*EUPBLD": "kindle",
    "HRAEULD": "ladle", // overrides "Long Island"
    "HRA*EULD": "ladle",
    "HRAOEUPBLD": "Long Island",
    "TPHAOLD": "noodle",
    "SAOEULD": "sidle",
    "SAO*EULD": "sidle",
    "SWEUPBLD": "swindle",
    "SW*EUPBLD": "swindle",
    "TRUPBLD": "trundle",
    "TR*UPBLD": "trundle",
    "WHAO*ELD": "wheedle", // WHAOELD is "wheeled"
    // As a special case, "handle" is HAPBL,
    // so HAPBLD makes more sense as "handled"

    // Miscellaneous star-dropping
    "KWRAER": "area",           // Too much mental gymnastics with KWRA*ER:
                                // "area" -> ERYA -> YAER -> YAER*
    "PWEUPBG": "binge",         // overrides "bing"
    "PW*EUPBG": "Bing",         // name of search engine
    "TK-G": "doing",            // swap with "diagnose"
    "TK-GS": "doings",          // swap with "diagnosis"
    "TK*G": "diagnose",
    "TK*GS": "diagnosis",
    "TP-RB": "efficiency",      // for consistency with TP-RBT "efficient"
    "AOEGS": "emotion",         // use SERBL for "essential"
    "TERPBL": "eternal",        // otherwise undefined
