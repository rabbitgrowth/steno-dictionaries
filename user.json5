    "T-TS": "it is the",
    "T*TS": "it's the",
    "T-FTS": "it was the",
    "TH-TS": "this is the", // use THA*S for "that's"
    "TH-FTS": "this was the",
    "THATS": "that is the",
    "THA*TS": "that's the",
    "THAFTS": "that was the",
    "WHATS": "what is the", // use WHA*S for "what's"
    "WHA*TS": "what's the",
    "WHAFTS": "what was the",
    "WH-TS": "when is the",
    "WH*TS": "when's the",
    "WH-FTS": "when was the",
    "W-RTS": "where is the",
