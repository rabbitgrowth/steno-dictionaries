    "WR*TS": "where's the",    // -R pushed to the left (unnecessarily)
    "WR-FTS": "where was the", // -R pushed to the left
    "WHOTS": "who is the",
    "WHO*TS": "who's the",
    "WHOFTS": "who was the",
    "KWR-TS": "why is the",
    "KWR*TS": "why's the",
    "KWR-FTS": "why was the",
    "-FTS": "was the",
    "W-RS": "where is",
    "KWR-S": "why is",

