    "SKREUF": "sacrifice", // "scriff"

    // -- Punctuation as shapes ------------------------------------------------

    // Ellipsis
    "-RGZ": "{^...}", // looks like three dots on the keyboard

    // Brackets
    // The closest thing that looks like an open bracket,
    // KPA, is already used for capitalizing the next word,
    // so use the next closest thing:
    //   P               T     L
    //  K   is used so  S       S
    //   A                A   U
    "STA": "{~|(^}", // open bracket
    "ULS": "{^~|)}", // close bracket

