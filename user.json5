{

    // Simpler vowels
    "PHERPL": "memory", // default is "moimr"
    "PHERPLS": "memories",

    // Use -L for -ly
    // END/-D "ended" feels nice. Why not F*UL/-L for "fundamentally"?
    "-L": "{^ly}",

    // Consistency
    "AFT": "after the",       // with "before the"
    "TPHOEU": "annoy",        // with "annoying", "annoyed"
    "PWRAOEUFPB": "brighten", // with the standard "frighten"
    "WOEPB": "woken",         // with "broken", "spoken"
    "SRAEL": "valley",        // with the "many" group; overrides "vale"
    "TOPL/KWREU": "Tommy",    // with "Sammy"

    // Reduce strokes for long words
    "SPWOBGS/KAEUGT": "intoxicating",
    "SPWREUBGT": "intricate",
    "PAPL/TAEUT": "palpitate",
    "STHRAOEUT": "satellite",
    "STRAOEUTD": "strident",
    "SRAPBL/TAOEUPB": "valentine",

    // Abbreviations outside of steno theories
    "SKWR-P": "Japan",     // JP
    "SKWR-PS": "Japanese",
    "KAOEB": "keyboard",   // keeb
    "SEBG": "second",      // sec
    "S*EBG": "sec",

    // Fold in A for a-
    "KRAOS": "across",
    "HAED": "ahead",
    "HRAEUF": "alive",
    "SAERT": "assert",
    "SAES": "assess",
    "SAET": "asset",
    "TAEPBD": "attend",

    // -URT for -utter
    "PWURT": "butter", // overrides "Burt"
    "KHRURT": "clutter",
    "TPHRURT": "flutter",
    "TKPWURT": "gutter",
    "PHURT": "mutter",

    // Omit F for -t-
    "TPRAOEUPB": "frighten",
    "TPRAOEUPBG": "frightening",
    "THREPB": "threaten", // threa(glottal stop)en
    "THREPBG": "threatening",
    "THREPBD": "threatened",

    // Remove unnecessary extra strokes
    "HUR": "hurry",
    "HURG": "hurrying",
    "HURD": "hurried",

    // Modal verb + be
    "PHAEUB": "may be",
    "PHAOEUBT": "might be",
    "PHUFBT": "must be",

    // Common adverbs
    "PRAOEUFLT": "privately",
    "PHREULG": "publicly",

    // More sensible plurals
    "TP-PBS": "finances",
    "HR-PBS": "licenses",

    // Add missing plurals
    "KAUPB/SKWREBG/TAOURS": "conjectures",
    "TKREGS": "dregs",
    "HRAUR/KWRATS": "laureates",
    "HRAUFS": "layoffs",
    "PHOD/RATS": "moderates",

    // Add -s after the right word
    "TOEFRTS": "efforts to",

    // Merge words in common expressions
    "HRAOFT": "a lot of",
    "SKPOER": "and other",
    "TPAS": "as if",
    "AOEPBT": "even the",
    "TPAOEPBT": "even if the",
    "STPH": "is in", // overrides ? but KW-PL is much nicer anyway
    "T-TS": "it's the",
    "TROEFR": "refer to",
    "TROEFRS": "refers to",
    "-FTS": "was the",
    "TWAOEU": "way to", // TWAOEUS already taken by "twice"
    "WHE": "when he",
    "TWH": "when it",

    // Current words
    "KROEPB/SRAOEURS": "coronavirus",

    // Don't map nice strokes to obscure words
    "PH-B": "maybe", // not "Mapbox"

    // Tricks
    "SED/SED": "said, \"{^}{-|}", // like SAEUD/SAEUD
    "HRAL": "{#command(a)}",      // lal -> all
    "TPHAOP": "{#}",              // JUJ/(no-op)/*MT -> judgement

    // O*ERBGS for "oh" is really ugly. I like the intuitiveness of
    // O -> oh, but it feels a bit wasteful to assign something as nice
    // as O to a word that's not super common. An alternative is "to".
    "O": "oh",

    // I hate KROEUR with every fiber of my being. Had to choose between
    // KR and K-R, both of which were taken up by "consider". Settled
    // with KR so that the visual lengths of the strokes match up with
    // the lengths of the words:
    // K-R   -> occur
    // K---R -> consider
    "KR": "occur",
    "KR-S": "occurs",
    "KR-G": "occurring",
    "KR-D": "occurred",

}
