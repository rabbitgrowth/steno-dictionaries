    "THA*EUL": "that I'll",  // use TH*EL for "they'll"
    "THAEULD": "that I would",
    "THAEUD": "that I'd",    // use THED for "they'd"
    "THAUBG": "that you can",
    "THAUBGD": "that you could",
    "THAURBD": "that you should",
    "THA*UL": "that you'll",
    "THAULD": "that you would",
    "THAUD": "that you'd", // by default "that you had"
    "THAEBG": "that he can", // use THAEBGT for "that's correct"
    "THAEBGD": "that he could",
    "THAERBD": "that he should",
    "THAEL": "that he will",
    "THA*EL": "that he'll",
    "THAELD": "that he would",
    "THAED": "that he'd",
    // Superimpose SHE for "she", cf. STHAE "that she" (THA + SHE)
    "STHAEFS": "that she was",
    "STHAEBG": "that she can",
    "STHAEBGD": "that she could",
    "STHAERBD": "that she should",
    "STHAEL": "that she will",
    "STHA*EL": "that she'll",
    "STHAELD": "that she would",
    // Special case: STHAED is more useful as "said that" than as "that she'd"

    // "what" stacking
    "WHAEUPL": "what I am",
    "WHA*EUPL": "what I'm",
    "WHAUR": "what you are",
    "WHA*UR": "what you're",
    "WHAES": "what he is",
    "WHA*ES": "what he's",
    "WHAEFS": "what he was",
    "WHAF": "what if",
    "WHAOER": "what other",

    // as + adjective + as
    // The most common ones already work beautifully:
    // AS WELL AS  155949  SW-LS / SW*ELS  (SWELS)
    // AS LONG AS   57042  SHROPBGS        (SLONGS)
    // AS MUCH AS   49462  SPHUFPS         (SMUCHS)
    // AS SOON AS   42046  SAOPBS          etc.
    // AS FAR  AS   41940  STPARS
    // AS GOOD AS   11733  STKPWAODZ
    // AS MANY AS   10931  SPHAEPBS
    // AS FAST AS    7145  STPAFTS
    "SPHALS": "as small as",

    // Miscellaneous phrasing strokes
    "PWHOU": "about how",
    "TPAS": "as if",
    "T-T": "it the", // as in "making it the..."
    "TPHOBT": "not be",
    "STHAO": "so that",
    "SAUFP": "such a",
    "TWH": "when it",
    "HR-F": "will have",

    // Plurals
    "PWEPB/TPEURBS": "beneficiaries", // just add -S instead of KWRAERS
    "KAUPB/SKWREBG/TAOURS": "conjectures",
    "TKEUPL/PHATS": "diplomats",
    "TKOPL/TPHOES": "dominoes",
    "TKREGS": "dregs",
    "EBG/OES": "echoes",
    "TP-PBS": "finances", // wasteful to have both TP-PB and TP-PBS for "finance"
    "SKWRUFSZ": "justices", // otherwise gives "justifieses" (SKWRUFZ + -S)
    "HRAUR/KWRATS": "laureates",
    "HRAUFS": "layoffs",
    "HR-PBS": "licenses",
    "PHOD/RATS": "moderates",
    "TPHARS/SEUFTS": "narcissists",
    "RAEU/SEUPBS": "raisins",
    "WEUPBD/TPAULS": "windfalls",
    "WEUPBD/PHEULS": "windmills",

    // -S vs -Z
    // In general, fold in -S to minimize the number of strokes:
    // KATS  "cats" (as opposed to KAT/-S)
    // TKOGS "dogs" (as opposed to TKOG/-S)
    // ...
    // When there's a conflict, use -S for -ce or -se (which forms an
    // integral part of the word) and -Z for -s (which is a removable
    // suffix). Mnemonic: -Z is further away and feels more detachable.
    // For example:
    // TPHOES "nose"
    // TPHOEZ "knows"
    // But always use -Z for -ze. For example:
    // SAOEUZ "size"
    // SAOEUS "sighs"
    "ROES": "rose", // swap with "rows"
    "ROEZ": "rows",
    "AOEUZ": "eyes", // overrides {^ize}
    "EUZ": "{^ize}",
    // When there's no conflict, prefer -S over -Z because it's easier
    // to type.
    "STKPAOEUS": "despise",
    "SKHRAUS": "Santa Claus",
    "THOUS": "thousand",

    // Inflections
    "ART/KHRAEUTD": "articulated",
    "ART/KHRAEUGT": "articulating", // otherwise gives "art calculating"
    "KAUPB/SAOELG": "concealing",
    "KAUPB/SAOELS": "conceals",
    "ES/KHRAEUTD": "escalated",
    "ES/KHRAEUGT": "escalating",
    "SPWOBGS/KAEUGT": "intoxicating",
    "PUPBGT/WAEUTD": "punctuated",
    "PUPBGT/WAEUGT": "punctuating",
    "RE/SEUFTD": "resisted",
    "RE/SEUFTS": "resists",
    "SHREUD/TPAOEUG": "solidifying",

    // Fold in -T for -ed if the verb already ends with -d
    // Precedents: TPHAOETD "needed", STKAOEUTD "decided"
    "TKPWAOEUTD": "guided",
    "HAPBTD": "handed",

    // Expand "n't" into "not" with -L as bridge
    "K-PBLT": "cannot",     // from K-PBT   "can't"
    "TKOEPBLT": "do not",   // from TKOEPBT "don't" (or just use the standard TKPHOT)
    "TKOPBLT": "does not",  // from TKOPBT  "doesn't"
    "PHUPBLT": "must not",  // from PHUPBT  "mustn't"
    "SHAPBLT": "shall not", // from SHAPBT  "shan't"
    "WOEPBLT": "will not",  // from WOEPBT  "won't" (or just use the standard HR-PBT)

    // Use -PBLT for "not" even when there's no -n't form
    // Precedent: KWR-PBLT "why not"
    "SKP-PBLT": "and not", // there's no "andn't"
    "ORPBLT": "or not",    // there's no "orn't"

    // Don't capitalize
    "OP/ED": "op-ed",
    "AUP/STAEUT": "upstate",

    // Pick more distinctive part of word
    "STRAEUGS": "administration",
    "STEPBD": "extend",      // overrides "send it"
    "STEPBD/-D": "extended", // misstroke for "tended"?
    "TKAO*ED": "indeed",
    "RAUPBT": "restaurant", // restau[rant] or [r]estaur[ant]
    "TPHAOEBG": "technique", // add * for "anemic"

    // Assign nicer stroke to word that's more common
    "STPHUR": "ensure", // merges TPH and SH
    "STPH*UR": "insure",

    // Existing abbreviations
    "TK-L": "download",    // dl
    "SKWR-P": "Japan",     // JP
    "SKWR-PS": "Japanese",
    "KAOEB": "keyboard",   // keeb
    "PHO": "month",        // mo
    "PH-FT": "movement",   // mvt
    "PREF": "previous",    // prev
    "SEBG": "second",      // sec
    "S*EBG": "sec",
    "T-GT": "together",    // tgt

    // Add * to expand to full form
    // Precedent: TPREUPBLG  "fridge"
    //            TPR*EUPBLG "refrigerator"
    // "Not an accident" -- Mirabai
    "TP*EFT": "festival",  // fest
    "HRA*B": "laboratory", // lab
    "PO*P": "popular",     // pop
    "P*UB": "publish",     // pub
    "P*URB": "publisher",
    "SR*ET": "veteran",    // vet

    // ... or double the short form when it already has a *
    "PHA*T/PHA*T": "mathematics", // PHA*T/-T and PHAT/PHAT are
                                  // still available for "Matt"

    // Missing words
    "ART/SEU/TPART/SEU": "artsy-fartsy",
    "KAUPB/SAOERL": "concealer",
    "TRAORP": "trooper",

    // Current words
    "KROEPB/SRAOEURS": "coronavirus",
    "KOEFD/1-9": "Covid-19",
    "PR*UPL": "President Trump",

    // Prevent word boundary errors
    "PAS/SO": "pass so", // overrides "Paso"; use EL/PA/SOE for "El Paso"

    // Special rules
    "SKW": "and",            // SKP with P one tile down
    "TAL": "at all",         // two outer letters stripped
    "WAU": "away",           // arbitrary
    "SHREPBD": "endless",    // SL as mirror image of -LS
    "SHREPBLD": "endlessly",
    "HOELT": "hotel",        // reverse stress or HOELT->HOTEL
    "TAOU": "in order to",   // a stronger "to"
    "KW-T": "quiet",         // when you speak quietly the vowels become harder to hear
    "TH-RS": "there is",     // spread out a bit to expand 's

    // Commands
    "TPHAOP": "{#}", // JUJ/(no-op)/*MT -> judgement
    "HRAL": "{#command(a)}", // lal -> all

    // Affirmation
    "KWRE": "yeah", // by default "yes"
    "KWRES": "yes", // by default "yes, sir"
    "KWREFS": "yes{,}sir", // "yess"

    // Amounts
    "AEULT": "a lot", // The -L in HROELT doesn't seem to be doing anything
    "AEUFLT": "a lot of",
    // Having "a lot of" makes HROFT "lot of" pretty useless because "lot of"
    // is almost always preceded by "a". Of course "a whole/awful/etc. lot of"
    // is possible, but "a lot of" is by far the most common:
    // A LOT OF     334111
    // WHOLE LOT OF   5508
    // AWFUL LOT OF   3317
    // HROFT is much more suited to ...
    "HROFT": "lost",
    // Since we fixed HRO*S "lost", we might as well also fix HR*EUS "list":
    "HREUS": "list", // overrides "Lys-", whatever that is
                     // think "drop *" or "ls"
    // HRAEUL "a little" already works nicely
    "HRAEUFL": "a little of",
    "HRAEUFLT": "a little of the",
    "HRAEUBLT": "a little bit",
    "HRAEUFBLT": "a little bit of", // completely flouts steno order:
                                    //    2 9 68
                                    //    3  7
                                    //   1  45
    "HRAEUBT": "a little bit", // drop -L to mitigate the violation a little bit ("alibit")
    "HRAEUFBT": "a little bit of",

