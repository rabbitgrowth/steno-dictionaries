    "AOEG": "ego",
    "SKWRAOUF": "juvenile",
    "RE/SKWRAOUF": "rejuvenate",
    "RE/SKWRAOUFGS": "rejuvenation",
    "TPHAR": "narrow",
    "SAOUF": "souvenir",
    "SREUG": "vigor", // I don't want "have Iing" flashing past every time
