{

    // Simpler vowels
    "PHERPL": "memory", // default is "moimr"
    "PHERPLS": "memories",

    // Use -L for -ly
    // END/-D "ended" feels nice. Why not F*UL/-L for "fundamentally"?
    "-L": "{^ly}",

    // Fold in A for a-
    "KRAOS": "across",
    "HAED": "ahead",
    "HRAEUF": "alive",
    "SAERT": "assert",
    "SAES": "assess",
    "SAET": "asset",
    "TAEPBD": "attend",

    // -URT for -utter
    "PWURT": "butter", // overrides "Burt"
    "KHRURT": "clutter",
    "TPHRURT": "flutter",
    "TKPWURT": "gutter",
    "PHURT": "mutter",

    // Omit F for -t-
    "TPRAOEUPB": "frighten",
    "TPRAOEUPBG": "frightening",
    "THREPB": "threaten", // threa(glottal stop)en
    "THREPBG": "threatening",
    "THREPBD": "threatened",

    // Consistency with -ing and -ed
    "TPHOEU": "annoy",

    // Remove unnecessary extra strokes
    "HUR": "hurry",
    "HURG": "hurrying",
    "HURD": "hurried",

    // More sensible plurals
    "TP-PBS": "finances",
    "HR-PBS": "licenses",

    // Add missing plurals
    "KAUPB/SKWREBG/TAOURS": "conjectures",
    "TKREGS": "dregs",
    "HRAUR/KWRATS": "laureates",
    "HRAUFS": "layoffs",
    "PHOD/RATS": "moderates",

    // Common adverbs
    "PRAOEUFLT": "privately",
    "PHREULG": "publicly",

    // Current words
    "KROEPB/SRAOEURS": "coronavirus",

}
