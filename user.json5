    // Use nicer prefixes for multistroke words
    "SPWR/TPAOER": "interfere",   // prefer SPWR to EUPBT
    "SPWR/TPAOERS": "interferes",
    "SPWR/TPAOERG": "interfering",
    "SPWR/TPAOERD": "interfered",
    "SPWR/TPAOERPBS": "interference",
    "SPR/STEUGS": "superstition", // prefer SPR to SAOUP
    "SPR/STEURBS": "superstitious",
    // There's something ugly about using {inter^} for "interfere",
    // because "fere" isn't a word -- you aren't tacking "inter-" onto
    // "fere". In fact, TPAOER is "fear", so SPWR/TPAOER feels a bit
    // like it should be "interfear". However, Plover often allows
    // using prefixes even when the part after the prefix isn't a word.
    // For example, although "ceal" isn't a word, for "conceal" Plover
    // allows (in addition to KOPB/SAOEL) KAUPB/SAOEL, which feels
    // natural because "con-" is so common KAUPB becomes muscle memory
    // pretty quickly.

    // Pronoun + be
    // Default to full form and add * or use phonetics for contraction
    "KWR-PL": "I am",  // overrides "I mean"
    "KWR*PL": "I mean",
    // "I'm"    AOEUPL already works
    "KWR-FS": "I was", // overrides "why was"
    "UR": "you are",
    "*UR": "you're",
    "ES": "he is", // there's still S*E for {^es}, which seems to be useless anyway
    // "he's"   HAOES  already works
    // "she is" SHES   already works
    // "she's"  SHAOES already works
    "T-S": "it is",
    // "it's"   T*S    already works
    // "is it"  ST     already works
    // "we are" WER    already works
    // "we're"  WAOER  already works
    "W*ER": "we're",
    "THER": "they are",
    "TH*ER": "they're",

    // Pronoun + have
    // The standard strokes are inconsistent:
    // KWROEU  "I don't"
    // KWROEUF "I don't have"
    // SREU    "I have" (?!)
    "KWR-F": "I have",
    // "I've"     AOEUF already works
    "SREU": "have I", // follow steno order
    "UF": "you have",
    "*UF": "you've",
    "KWRAOUF": "you've",
    // "have you" SRU already works
    // "he has"   use HEZ  (EZ is "easy")
    // "she has"  use SHEZ
    "WEF": "we have", // there's still WAEF for "weave",
                      // which is admittedly inconsistent with HREF "leave"
    // "we've"    W*EF and WAOEF already work
    "THEF": "they have",
    "TH*EF": "they've",
    "THAEUF": "they've",

    // Pronoun + modal verb
    "KWR-BG": "I can", // overrides misstroke for {,}
    "KWR-BGD": "I could",
    // "can I"    KEU already works
    // "could I"  KOEU is "coy"
    "KWR-RB": "I shall", // Precedents for using -RB for "shall":
                         // SHERB "she shall", WHORB "who shall"
    "KWR-RBD": "I should",
    // "shall I"  just use SHAL/EU
    // "should I" SHOEU already works
    "KWR-L": "I will",   // overrides "ideal" stroke #1
    // "I'll"     AOEUL already works
    // "will I"   HREU {^ly} feels too canonical to override
    "KWR-LD": "I would", // overrides "ideal" stroke #2
    // "I'd"      AOEUD already works
    "WOEU": "would I", // by default "I would"
    "EUD": "idea", // "I-D"
                   // overrides "id" (psychoanalysis term)
    "*EUD": "id",
    "EULD": "ideal", // overrides "I would", for which I now have KWR-LD
    "HREULD": "ideally",

    // "you can"    UBG already works
    // "you could"  UBGD already works
    // "can you"    KU already works
    // "could you"  KOU is "cow"
    "URB": "you shall", // by default "you're"
    // "you should" URBD already works
    // "shall you"  too rare to care
    "SHOU": "should you", // misstroke for "somehow"
    "UL": "you will",
    "*UL": "you'll",
    // "you would"  ULD already works
    // "you'd"      UD already works
    // "will you"   HRU already works
    // "would you"  WOU already works

    // The standard strokes are inconsistent:
    //  ELD  "he would"  (E for "he")
    // HELD  "held"      (H in spelling included)
    //  ERBD "herbed"    (H in spelling omitted)
    // HERBD "he should" (HE for "he")
    // Of course, the "h" in "held" is pronounced while the "h" in "herb"
    // can be silent. Still, it would be nice to be able to consistently use
    // EU for "I", U for "you", and E for "he".
    "EBG": "he can", // overrides "ebbing"
    "EBGD": "he could",
    "KE": "can he", // by default "he can"
    // "could he"  KOE is "co-"
    "ERB": "he shall",   // use HERB for "herb" (spell out silent H as in HOUR "hour")
    "ERBD": "he should", // and for "herbed" use ...
    "HERBD": "herbed",
    // "shall he"  too rare to care
    // "should he" SHOE is "show"
    // "he will"   EL is "El"
    // "he'll"     HAOEL is "heel"
    // "he would"  ELD already works
    "HAOED": "heed", // for consistency with "heel"
    // "will he"   HRE is "le"
    // "would he"  WOE is "woe"

    // Plover pretty consistently uses -L for "'ll". In accordance with
    // our "default to full form and add * for contraction" rule, change
    // them all to "will".
    // Plover is so consistent with this that TH-L gives "this'll",
    // a pretty rare form:
    // THIS 'LL    1735
    // THIS WILL  31337
    // Compare
    // I 'LL   386926
    // I WILL  147058
    "T-L": "it will",    // by default "it'll"
    "THEL": "they will", // by default "they'll"
    "TH*EL": "they'll",
    "TH-L": "this will", // by default "this'll"
    "THAL": "that will", // by default "that'll"
    "WHOL": "who will",
    "WHAL": "what will",
    "THABG": "that can", // misstroke for "thank"
    "T-RB": "it shall",
    "THERB": "they shall", // by default "they're"

    // Modal verb + be
    "PHAEUB": "may be", // overrides "maybe"
    "PH-B": "maybe",    // overrides "Mapbox", but who cares?
    // PHAOEUB "might be" works already
    "PHUFB": "must be", // for consistency with PHAOEUB
    // A good reason not to just stack PHUFT and -B (PHUFBT)
    // for "must be" is to reserve the -T for "the".
    "PHAEUBT": "may be the",
    "PHAOEUBT": "might be the",
    "PHUFBT": "must be the",
    "K-BT": "can be the",
    "KOBT": "could be the",
    "SH-BT": "shall be the",
    "SHOBT": "should be the",
    // HR-BT "will be the"  works already
    // WOBT  "would be the" works already

    // -FPLT for "must"
    // I'm pretty much settled with TP-PL for {.} for life,
    // so this one should be safe to override. -FPLT = -FMT = MST,
    // and four keys in a row feels emphatic ("you MUST...").
    "EUFPLT": "I must",
    "UFPLT": "you must",
    "EFPLT": "he must",
    "SHEFPLT": "she must",
    "WEFPLT": "we must",
    "THEFPLT": "they must",

    // I (+ don't) + common verb
    // The standard strokes are inconsistent:
    // KWROEU  "I don't"
    // KWROEPB "I don't know"
    // KWROFT  "I don't have the"
    // These seem to use OEU, OE, and O for "don't" respectively.
    // For consistency, always use OE, which is
    // 1) the "average" of the three;
    // 2) the vowel in "don't";
    // 3) a nice symmetrical shape.
    "KWROE": "I don't", // overrides "yo"
    "KWRO": "yo", // use SKWRO for {^o}
    // Mirabai also uses SKWR for suffixes (and SKWR* for infixes),
    // so I'm in good company. The nice thing here is that I can
    // follow the pattern and add ...
    "KWRA": "ya", // instead of KWRO*U
                  // use SKWRA for {^a}
    // "I know"       KWR-PB  already works
    // "I don't know" KWROEPB already works
    "KWR-PBG": "I think", // thi[nk]
    "KWROEPBG": "I don't think",
    "KWR-PBT": "I want",  // wa[nt]
    "KWROEPBT": "I don't want",
    // KWR-F  "I have" has been defined above
    "KWROEF": "I don't have",
    // KWR*PL "I mean" has been defined above
    "KWRO*EPL": "I don't mean",
    "KWR-BZ": "I say",
    "KWR-BS": "I said",

    // EU "I", U "you", E "he"
    // The standard strokes are inconsistent:
    // WHEU "when I"
    // WHU  "when you"
    // WHE  "when" (?!)
    "TPE": "if he", // overrides "fe", which isn't a word
                    // the only downside I can think of is that it causes
                    // noise when typing words like TPE/KUPBD "fecund":
                    // "if he" -> "fecund" instead of "fe" -> "fecund"
    "WHE": "when he", // use WH for "when"
    "EBS": "he said", // use EBZ for "ebbs"

    // Reflexive pronouns
