    // -FPLT for "must"
    // I'm pretty much settled with TP-PL for {.} for life,
    // so this one should be safe to override. -FPLT = -FMT = MST,
    // and four keys in a row feels emphatic ("you MUST...").
    "EUFPLT": "I must",
    "UFPLT": "you must",
    "EFPLT": "he must",
    "SHEFPLT": "she must",
    "WEFPLT": "we must",
    "THEFPLT": "they must",

    // I (+ don't) + common verb
    // The standard strokes are inconsistent:
    // KWROEU  "I don't"
    // KWROEPB "I don't know"
    // KWROFT  "I don't have the"
    // These seem to use OEU, OE, and O for "don't" respectively.
    // For consistency, always use OE, which is
    // 1) the "average" of the three;
    // 2) the vowel in "don't";
    // 3) a nice symmetrical shape.
    "KWROE": "I don't", // overrides "yo"
    "KWRO": "yo", // use SKWRO for {^o}
    // Mirabai also uses SKWR for suffixes (and SKWR* for infixes),
    // so I'm in good company. The nice thing here is that I can
    // follow the pattern and add ...
    "KWRA": "ya", // instead of KWRO*U
                  // use SKWRA for {^a}
    // "I know"       KWR-PB  already works
    // "I don't know" KWROEPB already works
    "KWR-PBG": "I think", // thi[nk]
    "KWROEPBG": "I don't think",
    "KWR-PBT": "I want",  // wa[nt]
    "KWROEPBT": "I don't want",
    // "I have" has been defined above
    "KWROEF": "I don't have",

    // EU "I", U "you", E "he"
    // The standard strokes are inconsistent:
    // WHEU "when I"
    // WHU  "when you"
    // WHE  "when" (?!)
    "TPE": "if he", // overrides "fe", which isn't a word
                    // the only downside I can think of is that it causes
                    // noise when typing words like TPE/KUPBD "fecund":
                    // "if he" -> "fecund" instead of "fe" -> "fecund"
    "WHE": "when he", // use WH for "when"
    "EBS": "he said", // use EBZ for "ebbs"
    // THAEUFS "that I was"
    // THAURP  "that you were"
    // THAEFS  "THAEFS" (?!)
    "THAEFS": "that he was",
    "STHAEFS": "that she was",
    "THAEBGD": "that he could",
    "THAERBD": "that he should",
    "THAELD": "that he would",
    "STHAEBGD": "that she could", // THA + SHE, cf. STHAE "that she"
    "STHAERBD": "that she should",
    "STHAELD": "that she would",
    "WHAEFS": "what he was",

    // Reflexive pronouns
    "O*URS": "ourselves", // "ourself" is too rare
    "OUR/SEFL": "ourself",
    "WUPB/SEFL": "oneself", // WUPBS is "once", W*UPBS is "one's"
    "THAO*EUS": "thyself", // following the "add *S for reflexive pronoun" pattern
    "TH*EPLS": "themself", // add * for special usage
    "SEFL": "self", // since all possibilities have been exhausted
                    // ("myself", "yourself", ... and even "thyself"),
                    // -self as a suffix becomes useless

    // Stack "to" as TO or O
    "SRO": "have to",
    "SROB": "have to be", // you can keep going
    "SROBT": "have to be the", // and going
    "STROB": "have to be", // STRO for "have to" is standard
    "STROBT": "have to be the",
    "SKPO": "and to",
    "SKPOB": "and to be",
    "SKPOBT": "and to be the",
    "STKPO": "and to", // just for completeness; SKPO feels cleaner
    "STKPOB": "and to be",
    "STKPOBT": "and to be the",
    "TKPWO": "go to", // use TKPW for "go"
    "TKPWOG": "going to", // "goes to" and "went to" already work
    "TPWO": "about to",
    "TPWOB": "about to be", // needs T to avoid conflict with "bob"
    "STOBT": "is to be the",
    "TOFS": "was to",
    "TOFBS": "was to be",
    "TOFBTS": "was to be the", // tricky to type, just for completeness
    "STKWRO": "just to", // with our custom J for "just"
    "STKWROB": "just to be",
    "STKWROBT": "just to be the",
    "TOBT": "to be the",
    "TROEFR": "refer to",
    "TROEFRS": "refers to",
    "TOEFRTS": "efforts to", // pluralize the right word
    "TWAOEU": "way to", // TWAOEUS already taken by "twice"
    "TAOUFBD": "used to be",

    // is/as a/the
    // My fingers can never remember SAZ "as the" and SA*Z "as a".
    // I also dislike SA*EU "is a"; four keys and * for three letters!
    // Solution: use S for "is", A for "a", -T for "the", and add *
    // to change "is" to "as".
    "SA": "is a",
    "SA*": "as a",
    // S-T already gives "is the"
    "S*T": "as the",
    "ST-FPLT": "St.{-|}", // gives St. without {-|} by default,
                          // which seems to be never useful
    "TPHA": "in a", // following the pattern

    // wh-word + -ever + the
    "HOUFRT": "however the",
    "WHAFRT": "whatever the",
    "WH-FRT": "whenever the",
    "WR-FRT": "wherever the",
    "WEUFRT": "whichever the",
    "WHOFRT": "whoever the",

    // J for "just"
    "SKWR": "just",
    "SKWR-T": "just the", // overrides "jury trial"
    "SKWR-LG": "just like", // starless -lk
    "SKWR-PBG": "just in case", // "junk" is SKWRUPBG; no * needed

    // SPH for "some"
                             // Default strokes are inconsistent:
    "SPH-F": "some of",      // SPHOF  "some of"
    "SPH-FT": "some of the", // SOFPLT "some of the"
    "SPH-R": "some are",
    "SPH-RP": "some were",

    // P for "people"
    // PAOEPL is definitely a bit clumsy for how common "people" is.
    // PEPL is better (although it's listed as a misstroke).
    // P is only slightly easier to type than PEPL but really shines
    // when you stack it.
    "P": "people",
    "PR": "people are",
    "P-R": "people are",
    "PR-PBT": "people aren't",
    "PR-PBLT": "people are not",
    "P-RP": "people were",
    "PWR-PBT": "people weren't",
    "PWR-PBLT": "people were not",
    "PWHO": "people who",
    "TPO": "people to",

    // "and" stacking
    // SKP is just begging to be stacked.
    // Look at this sea of possibilities:
    //   P~ ~~~~~
    // SK~~ ~~~~~
    //   ~~ ~~
    "SKPHEUS": "and his",
    "SKPHER": "and her",
    "SKPOER": "and other",
    "SKPHR": "and will",           // or use SKP-L
    "SKPHR-B": "and will be",      // cf. "will be"
    "SKPHR-BT": "and will be the", // cf. "will be the"

    // Miscellaneous phrasing strokes
    "TPAS": "as if",
    "AOEPBT": "even the",
    "TPAOEPBT": "even if the",
    "STPH": "is in",
    "T-T": "it the", // as in "making it the..."
    "T-TS": "it is the",
    "TPHOBT": "not be",
    "STHAO": "so that",
    "-FTS": "was the",
    "TWH": "when it",

    // Plurals
    "PWEPB/TPEURBS": "beneficiaries", // just add -S instead of KWRAERS
    "KAUPB/SKWREBG/TAOURS": "conjectures",
    "TKEUPL/PHATS": "diplomats",
    "TKREGS": "dregs",
    "EBG/OES": "echoes",
    "TP-PBS": "finances", // wasteful to have both TP-PB and TP-PBS for "finance"
    "SKWRUFSZ": "justices", // otherwise gives "justifieses" (SKWRUFZ + -S)
    "HRAUR/KWRATS": "laureates",
    "HRAUFS": "layoffs",
    "HR-PBS": "licenses",
    "PHOD/RATS": "moderates",
    "TPHARS/SEUFTS": "narcissists",
    "WEUPBD/TPAULS": "windfalls",
    "WEUPBD/PHEULS": "windmills",

    // Inflections
    "ART/KHRAEUGT": "articulating", // otherwise gives "art calculating"
    "ART/KHRAEUTD": "articulated",
    "KAUPB/SAOELS": "conceals",
    "KAUPB/SAOELG": "concealing",
    "ES/KHRAEUGT": "escalating",
    "ES/KHRAEUTD": "escalated",
    "SPWOBGS/KAEUGT": "intoxicating",
    "SHREUD/TPAOEUG": "solidifying",

    // Fold in -T for -ed if the verb already ends with -d
    // Precedents: TPHAOETD "needed", STKAOEUTD "decided"
    "TKPWAOEUTD": "guided",

    // Expand "n't" into "not" with -L as bridge
    "K-PBLT": "cannot",     // from K-PBT   "can't"
    "TKOEPBLT": "do not",   // from TKOEPBT "don't" (or just use the standard TKPHOT)
    "TKOPBLT": "does not",  // from TKOPBT  "doesn't"
    "PHUPBLT": "must not",  // from PHUPBT  "mustn't"
    "SHAPBLT": "shall not", // from SHAPBT  "shan't"
    "WOEPBLT": "will not",  // from WOEPBT  "won't" (or just use the standard HR-PBT)

    // Use -PBLT for "not" even when there's no -n't form
    // Precedent: KWR-PBLT "why not"
    "SKP-PBLT": "and not", // there's no "andn't"
    "ORPBLT": "or not",    // there's no "orn't"

    // Don't capitalize
    "AUP/STAEUT": "upstate",

    // Heavy abbreviations that somehow make sense
    "SKP-PB": "and then",
    "TWH-BGT": "when it comes to",

    // Pick more distinctive part of word
    "STRAEUGS": "administration",
    "STEPBD": "extend",      // overrides "send it"
    "STEPBD/-D": "extended", // misstroke for "tended"?
    "TKAO*ED": "indeed",
    "RAUPBT": "restaurant", // restau[rant] or [r]estaur[ant]
    "TPHAOEBG": "technique", // add * for "anemic"

    // Assign nicer stroke to word that's more common
    "STPHUR": "ensure", // merges TPH and SH
    "STPH*UR": "insure",

    // Use -S instead of -Z
    "STKPAOEUS": "despise",
    "SKHRAUS": "Santa Claus",
    "THOUS": "thousand",

    // Drop *
    "KWRAER": "area",           // Too much mental gymnastics with KWRA*ER:
                                // "area" -> ERYA -> YAER -> YAER*
    "PWEUPBG": "binge",         // overrides "bing"
    "PW*EUPBG": "Bing",         // name of search engine
    "SPWREBGS": "intersection", // no * in "intersect"
    "SKWRUF": "justify",        // no * in "justifiable", "justification", etc.
    "SKOLGS": "psychologist",   // "psychologies" is too rare
    "T-FGS": "television",      // "it wasing" doesn't make any sense
    "KWRUP": "yup",             // use AUP for {up^}
    "-PLT": "{^ment}",          // use TP-PL for {.}
    "THOEUPB": "{^,000}",       // also avoids 3/2/1/THO*EUPB outputting "32 1,000"

    "PAEUT": "participate", // partici[pate] or [p]articip[ate]
    "PA*EUT": "pate",
    "PAEUGS": "participation", // I can use -Z if I really need to pluralize "payings"
    // I'm okay with the * in PA*PBT "participant" because "pant" is a common word.
    // PARPT makes sense too.

    // Existing abbreviations
    "TK-L": "download",    // dl
    "SKWR-P": "Japan",     // JP
    "SKWR-PS": "Japanese",
    "KAOEB": "keyboard",   // keeb
    "PHO": "month",        // mo
    "PH-FT": "movement",   // mvt
    "SEBG": "second",      // sec
    "S*EBG": "sec",
    "T-GT": "together",    // tgt

    // Add * to expand to full form
    // Precedent: TPREUPBLG  "fridge"
    //            TPR*EUPBLG "refrigerator"
    // "Not an accident" -- Mirabai
    "HRA*B": "laboratory", // lab
    "PO*P": "popular",     // pop
    "SR*ET": "veteran",    // vet

    // ... or double the short form when it already has a *
    "PHA*T/PHA*T": "mathematics", // PHA*T/-T and PHAT/PHAT are
                                  // still available for "Matt"

    // Missing words
    "ART/SEU/TPART/SEU": "artsy-fartsy",
    "KAUPB/SAOERL": "concealer",
    "TRAORP": "trooper",

    // Current words
    "KROEPB/SRAOEURS": "coronavirus",
    "KOEFD/1-9": "Covid-19",
    "PR*UPL": "President Trump",

    // For work
    "HED/WORD": "headword",
    "HED/WORDZ": "headwords",

    // Prevent word boundary errors
    "PAS/SO": "pass so", // overrides "Paso"; use EL/PA/SOE for "El Paso"

    // Special mnemonics
    "KW-T": "quiet", // so quiet you can't hear the vowels
                     // shhh, KWAO*EUT is too loud
    "TH-RS": "there is", // spread out a bit to expand 's

    // Tricks
    "SED/SED": "said, \"{^}{-|}", // like SAEUD/SAEUD
    "HRAL": "{#command(a)}",      // lal -> all
    "TPHAOP": "{#}",              // JUJ/(no-op)/*MT -> judgement

    // Affirmation
    "KWRE": "yeah",
    "KWRES": "yes",
    "KWREFS": "yes{,}sir", // "yess"

    // Amounts
    "AEULT": "a lot", // The -L in HROELT doesn't seem to be doing anything
    "AEUFLT": "a lot of",
    // Having "a lot of" makes HROFT "lot of" pretty useless because "lot of"
    // is almost always preceded by "a". Of course "a whole/awful/etc. lot of"
    // is possible, but "a lot of" is by far the most common:
    // A LOT OF     334111
    // WHOLE LOT OF   5508
    // AWFUL LOT OF   3317
    // HROFT is much more suited to ...
    "HROFT": "lost",
    // Since we fixed HRO*S "lost", we might as well also fix HR*EUS "list":
    "HREUS": "list", // overrides "Lys-", whatever that is
                     // think "drop *" or "ls"

    // HRAEUL "a little" already works nicely
    "HRAEUFL": "a little of",
    "HRAEUFLT": "a little of the",
    "HRAEUBLT": "a little bit",
    "HRAEUFBLT": "a little bit of", // completely flouts steno order:
                                    //    2 9 68
                                    //    3  7
                                    //   1  45
    "HRAEUBT": "a little bit", // drop -L to mitigate the violation a little bit ("alibit")
    "HRAEUFBT": "a little bit of",

    // "criticism" family
    "KREUFPL": "criticism", // "critsim" already truncates the "ci". Why not go further?
    "KRAOEUZ": "criticize",
    "KREUL": "critical",
    "KR*EUL": "krill", // I don't care if "krill" is a perfect match phonetically;
                       // "critical" is way, way more common
    "KREUBG": "critic",
    "KREUBGS": "critics",
    "KR*EUBG": "crick", // use KAERBG for "characteristic"

    // "dismiss" family
    "TKPHEUS": "dismiss", // DIS/MIS -> DMIS (IS shared by D and M)
                          // inspired by the standard TKPHEUFD "dismissed"
                          // (maybe it's a misstroke though?)
    "TKPHEUFG": "dismissing",
    "TKPHEUFL": "dismissal",
    "TKPHEUFS": "dismissive",

    // mass-
    "PHASZ": "masses",   // originally misstroke (?) for "mass"
    "PHAFS": "massive",  // originally "masses"
    "PHEUFS": "missive", // originally "massive"

    // "superior" and "inferior"
    "SPOEURT": "superiority",
    "TPOEURPBT": "inferiority", // from TPOEURPB (mnemonic: "infor")
                                //  FF   N
                                //      RN
                                //    O II
    "TPOEUR": "inferior", // nicely symmetrical with SPOEUR "superior"
                          // but overrides "foyer"
    "TPO*EUR": "foyer",
    "TPOEURT": "inferiority",

    // The superrich
    "TROEUPB/AEUR": "trillionaire", // cf. "millionaire", "billionaire"
    "SO*EUPB/AEUR": "zillionaire",

    // I hate WA*EU for "away". I briefly considered WEU which felt like
    // A/WAEU with the two A's cancelling each other out, but I wasn't
    // ready to give up WEU for "which". Then I discovered that WA was
    // just "was", for which there are nicer strokes: -FS for consistency
    // with phrasing strokes, and WAS for phonetic completeness.
    "WA": "away",

    // I hate O*ERBGS for "oh". I like the intuitiveness of O -> oh,
    // although it feels a bit wasteful to assign something as nice as O
    // to a word that's not super common. An alternative is "to".
    "O": "oh",

    // I hate HOEUFLT for "healthy". There's the ugly all-purpose
    // disambiguator OEU again, and what does that F even stand for?
    // H*EULT is better: it feels related to H*ELT "health", and I can
    // think of it as either "H*ELT with E changed to EU" or as "HLTHI".
    "H*EULT": "healthy",

    // I hate KROEUR for "occur". Had to choose between KR and K-R,
    // which were both "consider". Settled with KR so that the visual
    // lengths of the strokes matched up with the lengths of the words:
    // K-R   -> occur
    // K---R -> consider
    "KR": "occur",
    "KR-S": "occurs",
    "KR-G": "occurring",
    "KR-D": "occurred",

    // STKERTD feels like "deserted" but gives the non-word "desserted"
    // since STKERT is "dessert". I could just remap STKERTD to "deserted",
    // but that would lead to inconsistency within the "desert" word family:
    // STKERT  "dessert"
    // STK*ERT "desert"
    // STKERTD "deserted"
    // The solution is to swap "desert" and "dessert". It's also nice to have
    // the lengths of the strokes match up with the lengths of the words.
    "STKERT": "desert", // I then get STKERTD "deserted" for free
    "STK*ERT": "dessert",

    // STREUS feels more like "industries" (STREU + -S for plural) than
    // "industrious". Of course I can remember to use -Z to pluralize
    // "industry", but that's one more thing to remember.
    "STREUS": "industries",
    "STROUS": "industrious",

    // The standard dictionary has TPHURGS for "nurse" and TPHURBGS for
    // "nursing". There seem to be an extra G in the former and an extra
    // B in the latter.
    "TPHURGS": "nursing",

    // SKREUFS for "sacrifice" is nice because it has all the consonants
    // in it, but it ends with -S, which makes "sacrifices" and "sacrificed"
    // awkward. So just drop the -S. I can pretend "scriff" is Ploverese
    // for "sacrifice".
    "SKREUF": "sacrifice",

}
