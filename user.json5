    "HRAOEUBG/HRAOEUBG": "like, \"{^}{-|}", // and I was like, "..."
    "SED/SED": "said, \"{^}{-|}",
    "THAUT/THAUT": "thought, \"{^}{-|}",

    // "criticism" family
    "KREUFPL": "criticism", // "critsim" already truncates the "ci". Why not go further?
    "KRAOEUZ": "criticize",
