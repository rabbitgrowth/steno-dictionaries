"SRAPBGS": "vanish",
"SRAPBGS/-Z": "vanishes",
"SRARPBGS": "varnish",
"SRARPBGS/-Z": "varnishes",

// -- Negative contractions without *PBT -------------------------------

"AEUPBT": "ain't", // give up "-aint"
"ARPBT": "aren't",
"HAPBT": "hadn't",
"SAEPBT": "isn't",
"PHAOEUPBT": "mightn't",
