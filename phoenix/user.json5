{

// -- -PBGS for -nish --------------------------------------------------

// Precedents:
// PUPBGS     "punish"
// STAUPBGS   "astonish"
// TPEUPBGS   "finish"
// TKPHEUPBGS "diminish"
// The last one is particularly nice because it's made up of five 2x2 blocks.

"PWAPBGS": "banish", // used in PWAPBGS/AE "banshee" but "banishy" is not a word
"PWROUPBGS": "brownish",
"PWURPBGS": "burnish",
"TKAEUPBGS": "Danish",
"TKPWARPBGS": "garnish",
"TKPWRAOEPBGS": "greenish",
"PHREPBGS": "plenish",
"SPAPBGS": "Spanish",
"TARPBGS": "tarnish",
"SRAPBGS": "vanish",
"SRARPBGS": "varnish",

"PWAPBGS/-Z": "banishes",
"PWURPBGS/-Z": "burnishes",
"TKAEUPBGS/-Z": "Danishes", // "chocolate Danishes"
"TKPWARPBGS/-Z": "garnishes",
"PHREPBGS/-Z": "plenishes",
"TARPBGS/-Z": "tarnishes",
"SRAPBGS/-Z": "vanishes",
"SRARPBGS/-Z": "varnishes",

// -- Negative contractions without *PBT -------------------------------

"AEUPBT": "ain't", // give up "-aint"
"ARPBT": "aren't",
"HAPBT": "hadn't",
"SAEPBT": "isn't",
"PHAOEUPBT": "mightn't",
"TPHAOEPBT": "needn't",
"SHAEPBT": "shan't",

"PHUPBT": "mustn't", // cf. TKUPBT "doesn't"; overrides "month"
"PHO*": "month", // star "mo" for "month" like how you star "ME" for "Maine"
"PHO*Z": "months",

// -- R-R entries they forgot to update --------------------------------

// Sometimes when they swapped the outlines for homophones,
// they forgot to update the R-R entries as well, so the R-R does nothing.

"HAUL/R-R": "haul",
"HRAOEUBL/R-R": "libel",
"SRAEUPB/R-R": "vein",
"WAEUFB/R-R": "wave",

}
